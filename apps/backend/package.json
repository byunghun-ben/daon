{
  "name": "@daon/backend",
  "version": "1.0.0",
  "description": "Daon Backend API Server",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "db:types": "supabase gen types typescript --project-id trpuautkgonnjjalbsec > src/types/supabase.ts",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull",
    "db:diff": "supabase db diff",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:seed": "supabase db seed",
    "ios": "pnpm dev"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.55.0",
    "@aws-sdk/client-s3": "^3.835.0",
    "@aws-sdk/s3-request-presigner": "^3.835.0",
    "@azure/identity": "^4.10.1",
    "@daon/shared": "workspace:*",
    "@supabase/supabase-js": "^2.50.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "firebase-admin": "^13.4.0",
    "helmet": "^8.1.0",
    "openai": "^5.8.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@daon/eslint-config": "workspace:*",
    "@daon/tsconfig": "workspace:*",
    "@solana/wallet-standard-features": "^1.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.2",
    "rimraf": "^6.0.1",
    "supabase": "^2.26.9",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3"
  }
}
